# set macro path
/control/macroPath macros/

#tracking verbose, 5 is the max level
#/tracking/verbose 5

#gps particle setting
/gps/particle mu-

#gps set the energy level
/gps/energy 2 GeV

#set the starting position and direction of the gps. 1 m above the material
/gps/position 0 0.5 0 m
/gps/direction 0 -1 0

#initial beam attempt
/run/beamOn 10
